# Docker Compose Environment Variables
# Copy this file to .env and update the values for your environment

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production-with-at-least-32-characters

# Database Configuration
MONGODB_ROOT_USERNAME=admin
MONGODB_ROOT_PASSWORD=password123
MONGODB_DATABASE=cloudblitz

# Development Database Configuration
MONGODB_DEV_ROOT_USERNAME=admin
MONGODB_DEV_ROOT_PASSWORD=devpassword
MONGODB_DEV_DATABASE=cloudblitz_dev

# API Configuration
BACKEND_PORT=5000
FRONTEND_PORT=3000

# CORS Configuration
CORS_ORIGIN=http://localhost:3000